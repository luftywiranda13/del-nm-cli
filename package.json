{
  "name": "del-nm-cli",
  "version": "1.0.5",
  "description": "Delete `node_modules` and lockfiles",
  "license": "MIT",
  "repository": "https://github.com/luftywiranda13/del-nm-cli",
  "author": {
    "name": "Lufty Wiranda",
    "email": "lufty.wiranda@gmail.com",
    "url": "https://www.luftywiranda.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "precommit": "remove-lockfiles",
    "lint": "xo",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "bin": {
    "del-nm": "cli.js",
    "del-nm-cli": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "dependencies": {
    "del-nm": "^2.2.0",
    "meow": "^4.0.0",
    "update-notifier": "^2.3.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.3",
    "execa": "^0.8.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "remove-lockfiles": "^1.4.2",
    "tempy": "^0.2.1",
    "xo": "^0.18.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "async-to-gen"
    }
  },
  "xo": {
    "space": true,
    "envs": [
      "jest"
    ]
  },
  "keywords": [
    "cli",
    "cli-app",
    "clean",
    "cleanup",
    "delete",
    "remove",
    "node_modules",
    "npm",
    "yarn",
    "package",
    "dependencies",
    "reinstall"
  ]
}
